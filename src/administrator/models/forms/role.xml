<?xml version="1.0" encoding="UTF-8"?>
<form>
	<fieldset>
		<field
			name="id"
			type="hidden"
			default="0"
			filter="int"
		/>
		<field
			name="name"
			type="text"
			size="40"
			class="inputbox"
			label="COM_SUBUSERS_FORM_LBL_ROLE_NAME"
			description="COM_SUBUSERS_FORM_DESC_ROLE_NAME"
			required="true"
			filter="string"
		/>
		<field
			name="client"
			type="text"
			size="40"
			class="inputbox"
			label="COM_SUBUSERS_FORM_LBL_ROLE_CLIENT"
			description="COM_SUBUSERS_FORM_DESC_ROLE_CLIENT"
			filter="string"
		/>
		<field 
			name="created_by" 
			type="hidden"
			class="readonly"
			readonly="true"
			filter="unset"
		/>
		<field 
			name="modified_by" 
			type="hidden"
			class="readonly"
			readonly="true"
			filter="unset"
		/>
		<field
			name="modified_date"
			type="hidden"
		/>
		<field
			name="created_date"
			type="hidden"
		/>
		<field 
			name="ordering" 
			type="hidden"
		/>
	</fieldset>
</form>